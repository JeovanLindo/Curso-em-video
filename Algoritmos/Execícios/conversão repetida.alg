algoritmo "semnome"

var
     n, d, r, c: real
inicio

     escreva ("para quantas pessoas são : ")
     leia (n)
     enquanto (c<n) faca
       c <- 1+c
       escreva ("digite quantos R$ ")
       leia (r)
       d <- r/5.18
     fimenquanto
     escreva ("Quantos dolares voces irao ter : ",d:5:1,"US$")
fimalgoritmo
